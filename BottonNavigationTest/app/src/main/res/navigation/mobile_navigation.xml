<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_Address">

    <fragment
        android:id="@+id/navigation_Address"
        android:name="com.example.bottonnavigationtest.ui.Address.AddressFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_address" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.bottonnavigationtest.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_Phone"
        android:name="com.example.bottonnavigationtest.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>